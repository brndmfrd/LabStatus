ul {
  list-style: none;
  word-wrap: break-word;
}

/* Pages */
.helmet {
  padding: -10px;
  margin-top: -10px;
  margin-left: -10px;
  margin-right: -10px;
  background-color: black;
  color: white;
}
.head {
  width: 50%;
  height: 150px;
  margin-left: 20%;
  text-align: center;
  color: white;
  background-color: black;
  clear: both;
  padding-top: 75px;
  margin-top: 0;
  font-family: "Palatino Linotype", "Book Antiqua", Palatino, serif;
}
.block {
  float: left;
  width: 75%;
  height: 100%;
  text-align: left;
  padding-left: 10px;
  background-color: white;
  font-family: "Palatino Linotype", "Book Antiqua", Palatino, serif;
}
.blocktitle {
  padding: 0px;
  margin-top: 0px;
  margin-left: 0px;
  margin-right: 0px;
  font-size: 30px;
  font-style: italic;
}
.rightpane{
  float: left;
  width: 20%;
  height: 100%;
  text-align: left;
  padding-left: 5px;
  background-color: white;
}

#A00{
  background: grey;
  width: 5%;
  height: 5%;
}
